{
  "name": "@querent/wasm-chord-web",
  "version": "0.1.0-alpha",
  "description": "WebAssembly LLM inference runtime for browsers - WebGPU optimized",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "wasm-pack build ../../crates/wasm-chord-runtime --target web --out-dir ../../bindings/js/pkg --features webgpu",
    "build:ts": "tsc src/index.ts --outDir dist --target es2020 --module esnext --moduleResolution bundler --declaration",
    "test": "echo \"Web tests not yet implemented\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "wasm",
    "webassembly", 
    "llm",
    "inference",
    "ai",
    "machine-learning",
    "webgpu",
    "browser",
    "transformer"
  ],
  "author": "Querent AI <team@querent.xyz>",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/querent-ai/wasm-chord"
  },
  "files": [
    "dist/",
    "pkg/",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
